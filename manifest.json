{
	"manifest_version": 2,
	"name": "Amazon WishList Point",
	"version": "1.1",

	"description": "Add a function to display points on the Amazon wish list",
	"content_scripts": [
		{
			"matches": [
				"https://www.amazon.co.jp/gp/registry/wishlist/*"
			],
			"js": ["content_script.js"]
		}
	],
	"icons":{
		"128" : "ap128.png"
	}
}
